# vim: sw=2 sts=2 et syntax= :

param int @bgcolor 0
param int @fgcolor 15
param int @char 240
param bool @physblock false
param pos @targetpos thispos
global pos $teleportpos
global dir $pushdir
global entity $touchsrc

on state idle {
}

on event touch {
  say "Touched."
  set $teleportpos @targetpos
  send $touchsrc teleport
}
